@model List<Brand>

    <h1>Brands</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var brand in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => brand.Name)</td>
            </tr>
        }
        </tbody>
    </table>